<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>RealProject</display-name>
  <servlet>
    <servlet-name>ValidateCode</servlet-name>
    <servlet-class>com.blackstar0412.web.servlet.ValidateCode</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StaffLogin</servlet-name>
    <servlet-class>com.blackstar0412.web.servlet.StaffLogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetUserLogoById</servlet-name>
    <servlet-class>com.blackstar0412.web.servlet.GetUserLogoById</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>ValidateCode</servlet-name>
    <url-pattern>/web/servlet/validatecode</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StaffLogin</servlet-name>
    <url-pattern>/web/servlet/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetUserLogoById</servlet-name>
    <url-pattern>/web/servlet/getuserlogobyid</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>login.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>